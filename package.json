{
  "name": "camping-spot-publisher",
  "version": "1.0.0",
  "description": "AI-powered camping spot recommendation system with automatic note.com publishing",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "test": "node src/main.js --test",
    "publish-once": "node src/main.js --once",
    "dev": "node src/main.js",
    "area-master": "node generate-area-master.js",
    "update-otaru": "node update-otaru-data.js",
    "generate-otaru-html": "node generate-otaru-html.js",
    "generate-otaru-map": "node generate-otaru-map.js",
    "update-omoromachi": "node update-omoromachi-data.js",
    "generate-omoromachi-html": "node generate-omoromachi-html.js",
    "generate-omoromachi-map": "node generate-omoromachi-map.js",
    "omoromachi-all": "npm run update-omoromachi && npm run generate-omoromachi-map && npm run generate-omoromachi-html",
    "extract-regions": "node extract-region-data.js",
    "generate-index": "node generate-index-html.js",
    "build-index": "npm run extract-regions && npm run generate-index",
    "generate-all-regions": "node generate-all-regions.js",
    "generate-all-regions-full": "node generate-all-regions-full.js",
    "test-region-full": "node generate-all-regions-full.js --test"
  },
  "keywords": ["camping", "car-camping", "note", "supabase", "ai"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.2",
    "dotenv": "^17.2.2",
    "geolib": "^3.3.4",
    "node-cron": "^4.2.1",
    "node-fetch": "^2.7.0",
    "sharp": "^0.34.4"
  }
}